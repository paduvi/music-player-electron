{
  "name": "soundcloud-player",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/electron-starter.js",
  "productName": "SoundCloudPlayer",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "electron": "^1.6.10",
    "electron-packager": "^8.7.0",
    "foreman": "^2.0.0",
    "react-autocomplete": "^1.5.5",
    "react-scripts": "1.0.7",
    "react-sound": "^0.6.1"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-scripts start",
    "package-mac": "react-scripts build && electron-packager . --platform=darwin --overwrite --prune=true --out=dist --ignore=\"node_modules\" --icon=./public/icon/mac/logo.icns",
    "package-win": "react-scripts build && electron-packager . --platform=win32 --overwrite --asar=true --prune=true --out=dist --ignore=\"node_modules\" --icon=./public/icon/win/logo.ico",
    "package-linux": "react-scripts build && electron-packager . --platform=linux --overwrite --prune=true --out=dist --ignore=\"node_modules\" --icon=./public/icon/png/logo.png"
  }
}
